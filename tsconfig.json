{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "jsx": "react",
    "paths": {"~*": ["./src/*"]},
    "skipLibCheck": true /* Skip type checking all .d.ts files. */,
    "useDefineForClassFields": true,
    "target": "es5",
    "baseUrl": ".",
    "paths": {"~*": ["./src/*"]},
    "types": ["node"],
    "lib": ["ES2021.String"]
  },
  "exclude": ["node_modules"]
}
